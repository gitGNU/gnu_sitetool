me="sxml-sxml"

INPUT="$1"
if test X"$INPUT" = X"" ; then
    echo "$me: Missing input file"
    exit 1
fi

OUTPUT="$2"
if test X"$OUTPUT" = X"" ; then
    echo "$me: Missing output file"
    exit 1
fi

shift
shift

cp $INPUT $OUTPUT || {
    echo "$me: Error processing \`$INPUT'"
    rm -f $OUTPUT
    exit 1
}

exit 0
